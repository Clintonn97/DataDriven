public class testngdemo {
	@BeforeSuite
	public void A(){
		System.out.println("selenium");
	
	}
	@BeforeTest
    public void B(){
    	System.out.println("testng");
    	
    	
    }
	@BeforeClass
    public void c() {
		System.out.println("api");
		
    	
    }
	@BeforeMethod
   public void d() {
		System.out.println("maven");
		
	   
   }
	@Test
   public void e() {
		System.out.println("ecclipse");
		
	   
   }
	@AfterMethod
   public void f() {
		System.out.println("java");
		
	   
	   
   }
}
    
  